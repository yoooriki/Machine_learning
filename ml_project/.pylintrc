[MASTER]
disable=
    C0111, # missing-docstring
    C0103, # invalid-name
    C0303, # trailing-whitespace
    C0304, # missing-final-newline
    W0621, # redefined-outer-name
    R0903, # too-few-public-methods
    R0913, # too-many-arguments

[FORMAT]
max-line-length=120

[MESSAGES CONTROL]
disable=
    fixme,
    invalid-name,
    missing-docstring
